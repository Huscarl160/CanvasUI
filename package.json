{
  "name": "canvasui",
  "version": "0.0.0",
  "description": "A Three.JS WebXR UI. Enabling easy UI creation for immersive-vr sessions.",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "start": "node node_modules/@eviltik/devsrv/bin/devsrv.js",
    "prod": "node node_modules/@eviltik/devsrv/bin/devsrv.js -d dist/ -o",
    "build": "node node_modules/@eviltik/devsrv/bin/devsrv.js -b && node node_modules/rollup/dist/bin/rollup -c ./rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NikLever/CanvasUI.git"
  },
  "keywords": [
    "threejs",
    "webxr",
    "canvas",
    "ui",
    "gui"
  ],
  "author": "Nik Lever",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/NikLever/CanvasUI/issues"
  },
  "homepage": "https://github.com/NikLever/CanvasUI#readme",
  "dependencies": {
  },
  "devDependencies": {
    "@babel/preset-env": "7.16.11",
    "@eviltik/devsrv": "1.5.2",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@rollup/plugin-babel": "5.3.1",
    "fs-extra": "10.0.1",
    "glob": "7.2.0",
    "rollup": "2.69.1",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-terser": "7.0.2"
  }
}
